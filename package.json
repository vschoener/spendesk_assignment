{
  "name": "spendesk",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run serve",
    "flow": "flow",
    "build": "npm run lint && npm run build:transpile",
    "build:transpile": "babel src -d dist",
    "watch": "concurrently -k -p '[{name}]' -n 'Build,Node' -c 'green.bold,blue.bold' 'npm run build:transpile -- --watch' 'nodemon dist/main.js'",
    "serve": "node dist/main.js",
    "test": "jest --forceExit",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "apidoc": "apidoc -i src/api -o apidoc"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-mocha": "^5.0.0",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "joi": "^13.4.0",
    "mongoose": "^5.1.4",
    "mongoose-long": "^0.2.1",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-custom": "^1.0.5",
    "validator": "^10.3.0",
    "winston": "^2.4.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/docker/",
      "/dist/",
      "logs"
    ],
    "testRegex": "./test/.*.test.js$"
  }
}
